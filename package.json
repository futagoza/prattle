{
    "name": "prattle",
    "version": "0.0.0",
    "description": "Run spec, benchmark and impact tests, all from one tool",
    "keywords": [
        "framework",
        "test",
        "spec",
        "benchmark",
        "impact"
    ],
    "homepage": "https://prattle.now.sh/",
    "repository": "futagoza/prattle",
    "license": "MIT",
    "author": "Futago-za Ryuu <futagoza.ryuu@gmail.com>",
    "files": [
        "bin",
        "dist",
        "src/*/"
    ],
    "main": "dist/node/api/index.js",
    "bin": "bin/prattle",
    "browser": "dist/prattle.js",
    "scripts": {
        "lint": "eslint --ext .js,.ts .",
        "spec": "mocha --reporter progress",
        "build:node": "rimraf dist/node && tsc -p src",
        "build:browser": "wp --config src/webpack.config.js",
        "clean": "rimraf .nyc_output coverage dist",
        "test": "nyc mocha",
        "coverage": "codecov -f coverage/*.json",
        "prepack": "npm run build:node && npm run build:browser",
        "postversion": "git push --follow-tags && npm publish"
    },
    "dependencies": {
        "@sinonjs/referee-sinon": "5.0.0",
        "benchmark": "2.1.4",
        "bluebird": "3.7.2",
        "clone-deep": "4.0.1",
        "colourant": "1.1.0",
        "cosmiconfig": "6.0.0",
        "dateformat": "3.0.3",
        "dedent": "0.7.0",
        "easy-table": "1.1.1",
        "fs-extra-plus": "0.5.12",
        "prompts": "2.3.0",
        "resolve": "1.13.1",
        "semver": "6.3.0",
        "sisteransi": "1.0.4"
    },
    "devDependencies": {
        "@babel/core": "7.7.5",
        "@babel/preset-env": "7.7.6",
        "@babel/preset-typescript": "7.7.4",
        "@futagoza/eslint-config": "12.0.0",
        "@futagoza/tsconfig": "1.0.0",
        "@types/clone-deep": "4.0.1",
        "@types/dateformat": "3.0.1",
        "@types/node": "10",
        "babel-loader": "8.0.6",
        "codecov": "3.8.3",
        "eslint": "6.7.2",
        "mocha": "6.2.2",
        "nyc": "14.1.1",
        "rimraf": "3.0.0",
        "ts-loader": "6.2.1",
        "ts-node": "8.5.4",
        "typescript": "3.7.3",
        "webpack": "4.41.2",
        "webpack-nano": "0.8.1"
    },
    "optionalPeerDependencies": {
        "@babel/core": "^7.7.0",
        "@babel/preset-env": "^7.7.0",
        "@babel/preset-typescript": "^7.7.0",
        "babel-loader": "^8.0.0",
        "ts-loader": "^6.2.0",
        "typescript": "^3.7.0",
        "webpack": "^4.41.0"
    },
    "engines": {
        "node": ">= 10"
    }
}
